{
	"name": "gstatem-workspace",
	"description": "GStatem is a small, simple and fast state-management tool.",
	"private": true,
	"scripts": {
		"build": "lerna exec -- yarn build",
		"clean": "lerna exec -- yarn clean",
		"test": "jest",
		"storybook": "yarn gen_sb_js && start-storybook -p 6006 -c ./storybook --ci",
		"build-storybook": "yarn build && yarn gen_sb_js && build-storybook -c ./storybook",
		"pub": "yarn build && lerna publish --bump=patch --yes && yarn clean",
		"updep": "yarn add -D -W gstatem-devtools@latest gstatem@latest react-gstatem@latest solid-gstatem@latest",
		"pack-extension": "yarn build && lerna run build-extension && ts-node ./packages/gstatem-devtools/pack-extension",
		"gen_sb_js": "ts-node ./storybook/gen_js"
	},
	"author": "shuieryin",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/gstatem/gstatem.git"
	},
	"homepage": "https://github.com/gstatem/gstatem",
	"bugs": {
		"url": "https://github.com/gstatem/gstatem/issues"
	},
