{
	"name": "gstatem-devtools",
	"description": "Gstatem devtools",
	"version": "1.0.0-alpha.13",
	"author": "shuieryin",
	"main": "dist/index.js",
	"scripts": {
		"check-types": "tsc --project ./src --declaration -outDir dist",
		"babel": "babel --config-file ../../babel.config.js dist -d dist --verbose",
		"build": "rimraf dist && yarn check-types && yarn babel && rollup -c && rimraf dist/background dist/pageScript/pageScript*",
		"build-extension": "rimraf dist extension/dist && webpack --config webpack.prod.js --progress",
		"pack-extension": "yarn build-extension && mkdir -p ../../dist && rimraf ../../dist/gstatem-devtools.zip && cd ./extension && zip ../../../dist/gstatem-devtools.zip ./*",
		"clean": "rimraf extension/dist dist node_modules package-lock.json",
		"start": "yarn clean && webpack-dev-server"
	},
	"dependencies": {
		"gstatem": "^1.0.0-alpha.11"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/gstatem/gstatem.git"
	},
	"homepage": "https://github.com/gstatem/gstatem/tree/main/packages/gstatem-devtools",
	"bugs": {
		"url": "https://github.com/gstatem/gstatem/issues"
	},
	"keywords": [
		"react",
		"state",
		"management",
		"redux",
		"zustand",
		"store",
		"statem",
		"gstatem"
	]
}
