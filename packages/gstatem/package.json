{
	"name": "gstatem",
	"description": "A light weight global state management library",
	"version": "1.0.0-alpha.6",
	"author": "shuieryin",
	"main": "dist/index.js",
	"scripts": {
		"check-types": "tsc --project ./ -outDir dist",
		"babel": "babel --config-file ../../babel.config.js dist -d dist --verbose",
		"build": "yarn clean && yarn check-types && yarn babel",
		"clean": "rimraf dist node_modules package-lock.json"
	},
	"keywords": [
		"react",
		"state",
		"management",
		"redux",
		"zustand",
		"store",
		"statem",
		"gstatem"
	]
}
