{
	"compilerOptions": {
		"sourceMap": false,
		"isolatedModules": false,
		"esModuleInterop": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"target": "ESNext",
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"jsx": "preserve",
		"declaration": true
	},
	"exclude": [
		"./node_modules",
		"./dist"
	]
}
