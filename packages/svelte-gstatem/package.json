{
	"name": "svelte-gstatem",
	"description": "A light weight global state management library for Svelte JS",
	"version": "1.0.0-alpha.1",
	"author": "shuieryin",
	"main": "dist/index.js",
	"scripts": {
		"check-types": "ts-node ../../check-types",
		"babel": "babel --config-file ../../babel.config.js dist -d dist --verbose",
		"build": "yarn clean && yarn check-types && yarn babel",
		"clean": "rimraf dist node_modules package-lock.json"
	},
	"peerDependencies": {
		"svelte": ">=3.0.0"
	},
	"dependencies": {
		"gstatem": "^1.0.10"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/gstatem/gstatem.git"
	},
	"homepage": "https://github.com/gstatem/gstatem/tree/main/packages/svelte-gstatem",
	"bugs": {
		"url": "https://github.com/gstatem/gstatem/issues"
	},
	"keywords": [
		"react",
		"solid",
		"vue",
		"svelte",
		"state",
		"management",
		"redux",
		"recoil",
		"pinia",
		"vuex",
		"mobx",
		"zustand",
		"store",
		"statem",
		"gstatem"
